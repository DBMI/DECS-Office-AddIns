<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
	<ribbon>
		<tabs>
			<tab id="DecsTab" label="UCSD Data Tools">
				<group id="FormatGroup"
					   label="Format">
					<button id="formatSheet"
							getImage="formatButton_GetImage"
							label="Format Results"
							onAction="OnFormat"
							screentip="Formats current sheet"
							size="large"
							supertip="Nice formatting for results sheet." />
					<button id="copyFormatting"
							getImage="copyFormatButton_GetImage"
							label="Copy Formatting"
							onAction="OnCopyFormat"
							screentip="Copies formatting from current sheet to next"
							size="large"
							supertip="Copies formatting to next sheet." />
					<button id="convertDate"
							getImage="dateConvertButton_GetImage"
							label="Convert Dates"
							onAction="OnConvertDates"
							screentip="Convert MUMPS Dates"
							size="large"
							supertip="Adds new column with converted dates." />
				</group>
				<group id="ListGroup"
					   label="Lists">
					<button id="buildListButton"
							getImage="buildListButton_GetImage"
							label="Import List"
							onAction="OnBuildList"
							screentip="Build list import code"
							size="large"
							supertip="Build SQL code to import a list." />
				</group>
				<group id ="MergeGroup"
					   label="Merge">
					<button id="mergeRowButton"
							getImage="mergeRowsButton_GetImage"
							label="Merge Rows"
							onAction="OnMergeRows"
							screentip="Merge rows"
							size="large"
							supertip="Merge results." />
				</group>
				<group id="NotesGroup"
					   label="Search Notes">
					<button id="searchButton"
							getImage="notesSearchButton_GetImage"
							label="Search Notes"
							onAction="OnSearchNotes"
							screentip="Search Notes"
							size="large"
							supertip="Search notes using existing config file." />
					<button id="setupButton"
							getImage="notesConfigButton_GetImage"
							label="Setup Config"
							onAction="OnSearchConfig"
							screentip="Configure Search"
							size="large"
							supertip="Configure search of notes." />
					<button id="mergeNotesButton"
							getImage="mergeNotesButton_GetImage"
							label="Merge Notes"
							onAction="OnMergeNotes"
							screentip="Merge notes"
							size="large"
							supertip="Merge results on index." />
				</group>
				<!--
				<group id ="SignalGroup"
					   label="Epic Signal">
					<button id="extractTime"
							getImage="timeInNotesButton_GetImage"
							label="Extract Time-In-Notes"
							onAction="OnTimeInNotes"
							screentip="Extracts data from Epic Signal download"
							size="large"
							supertip="Extracts Time in Notes from Epic Signal download" />
				</group>
				-->
				<group id ="SviGroup"
					   label="SVI">
					<button id="addSVI"
							getImage="sviButton_GetImage"
							label="Add SVI"
							onAction="OnAddSVI"
							screentip="Looks up Social Vulnerability Index"
							size="large"
							supertip="Gets Census Tract from address, then looks up SVI from .csv file." />
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>

