<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
	<ribbon>
		<tabs>
			<tab id="DecsTab" label="UCSD Data Tools">
				<group id="FormatGroup"
					   label="Format">
					<button id="formatSheet"
							getImage="formatButton_GetImage"
							label="Format Results"
							onAction="OnFormat"
							screentip="Format current sheet"
							size="large"
							supertip="Nice formatting for results sheet" />
					<button id="copyFormatting"
							getImage="copyFormatButton_GetImage"
							label="Copy Formatting"
							onAction="OnCopyFormat"
							screentip="Copy formatting from current sheet to next"
							size="large"
							supertip="Copy formatting from current sheet to all remaining sheets" />
					<button id="convertDate"
							getImage="dateConvertButton_GetImage"
							label="Convert Dates"
							onAction="OnConvertDates"
							screentip="Convert MUMPS Dates"
							size="large"
							supertip="Adds new column with converted dates" />
					<button id="convertDateToText"
							getImage="dateToTextButton_GetImage"
							label="Convert Dates to Text"
							onAction="OnDatesToText"
							screentip="Convert Dates to Text"
							size="large"
							supertip="Adds new column with text version of dates" />
					<button id="stripe"
							getImage="stripeButton_GetImage"
							label="Stripe by Value"
							onAction="OnStripe"
							screentip="Stripe by data"
							size="large"
							supertip="Stripe worksheet by data value" />
					<button id="count"
							getImage="countWordsButton_GetImage"
							label="Count Words/Char"
							onAction="OnCountWords"
							screentip="Count Words and Char"
							size="large"
							supertip="Count words and characters"/>
				</group>
				<group id="GrouchoGroup"
					   label="Deidentify">
					<button id="deidentify"
							getImage="deidentifyButton_GetImage"
							label="Deidentify"
							onAction="OnDeidentify"
							screentip="Deidentify"
							size="large"
							supertip="Generate hash from selected column" />
					<button id="hideDateTime"
							getImage="hideDateTimeButton_GetImage"
							label="Obscure Date/Time"
							onAction="OnObscureDateTime"
							screentip="Obscure Date and Time"
							size="large"
							supertip="Adds random offset to dates, times" />
					<button id="hidePhysicianNames"
							getImage="hidePhysicianNamesButton_GetImage"
							label="Hide Dr. Names"
							onAction="OnHidePhysicianNames"
							screentip="Hide Physician Names"
							size="large"
							supertip="Convert Physician Names to Scrambled ID" />
				</group>
				<group id="ListGroup"
					   label="Lists">
					<button id="buildListButton"
							getImage="buildListButton_GetImage"
							label="Import List"
							onAction="OnBuildList"
							screentip="Build list import code"
							size="large"
							supertip="Build SQL code to import a list" />
					<button id="signalImportButton"
							getImage="signalImportButton_GetImage"
							label="Import Signal Data"
							onAction="OnSignalImport"
							size="large"
							screentip="Import Signal data from JSON file"/>
					<button id="chopIntoTabsButton"
							getImage="chopIntoTabsButton_GetImage"
							label="Separate Tabs"
							onAction="OnChopList"
							screentip="Break up into separate tabs"
							size="large"
							supertip="Break up long list into separate tabs by category" />
					<button id="matchPhysiciansButton"
							getImage="matchPhysiciansButton_GetImage"
							label="Match Physicians"
							onAction="OnMatchPhysicians"
							screentip="Assign record # to physicians"
							size="large"
							supertip="Apply record numbers assigned in one sheet to another sheet" />
					<button id="lookupNpiButton"
							getImage="lookupNpiButton_GetImage"
							label="Lookup NPI"
							onAction="OnLookupNpi"
							screentip="Lookup NPI"
							size="large"
							supertip="Lookup National Provider Identifier" />
					<button id="onCallListButton"
							getImage="onCallListButton_GetImage"
							label="On-Call List"
							onAction="OnProcessCallList"
							screentip="Extract on-call list"
							size="large"
							supertip="Turn physician on-call list into SQL-friendly version" />
					<button id="searchByEmailButton"
							getImage="searchByEmailButton_GetImage"
							label="Search by Email"
							onAction="OnSearchByEmail"
							screentip="Search Providers by Email"
							size="large"
							supertip="Search Providers by Email" />
					<button id="sortTimesButton"
							getImage="sortTimesButton_GetImage"
							label="Sort Follow-up Times"
							onAction="OnSortTimes"
							size="large"
							screentip="Sort Times into High, Medium and Routine Priorities"/>
					<button id="sortTimesSettingsButton"
							getImage="sortTimesSettingsButton_GetImage"
							label="Choose Thresholds"
							onAction="OnSortTimesSettings"
							size="large"
							screentip="Choose Thresholds for Follow-up Times"/>
				</group>
				<group id ="MergeGroup"
					   label="Merge">
					<button id="extractTextButton"
							getImage="extractTextButton_GetImage"
							label="Extract Text"
							onAction="OnExtractText"
							screentip="Extract text"
							size="large"						
							supertip="Extract text from REDCap HTML" />
					<button id="matchTextButton"
							getImage="matchTextButton_GetImage"
							label="Match Text"
							onAction="OnMatchText"
							screentip="Match text"
							size="large"
							supertip="Match text" />
					<button id="mergeRowButton"
							getImage="mergeRowsButton_GetImage"
							label="Merge Rows"
							onAction="OnMergeRows"
							screentip="Merge rows"
							size="large"
							supertip="Merge results" />
					<button id="mergeFilesButton"
							getImage="mergeFilesButton_GetImage"
							label="Merge Files"
							onAction="OnMergeFiles"
							screentip="Merge files"
							size="large"
							supertip="Merge .csv files" />
					<button id="addRowsAcrossSheetsButton"
							getImage="addRowsAcrossSheets_GetImage"
							label="Add Rows Across Sheets"
							onAction="OnAddRowsAcrossSheets"
							screentip="Add Rows Across Sheets"
							size="large"
							supertip="Add Rows Across Sheets" />
				</group>
				<group id="NotesGroup"
					   label="Search Notes">
					<button id="searchButton"
							getImage="notesSearchButton_GetImage"
							label="Search Notes"
							onAction="OnSearchNotes"
							screentip="Search Notes"
							size="large"
							supertip="Search for keywords within notes" />
					<button id="setupButton"
							getImage="notesConfigButton_GetImage"
							label="Setup Config"
							onAction="OnSearchConfig"
							screentip="Configure Search"
							size="large"
							supertip="Configure search of notes" />
					<button id="mergeNotesButton"
							getImage="mergeNotesButton_GetImage"
							label="Merge Notes"
							onAction="OnMergeNotes"
							screentip="Merge notes"
							size="large"
							supertip="Merge results on index" />
					<button id="extractMessageButton"
							getImage="extractMessageButton_GetImage"
							label="Extract Message"
							onAction="OnExtractMessage"
							screentip="Extract Msg to Pt"
							size="large"
							supertip="Extract Message to Patient" />
				</group>
				<group id ="SurveyGroup"
					   label="Survey">
					<button id="importSurveyButton"
							getImage="importSurveyButton_GetImage"
							label="Import Survey"
							onAction="OnImportSurvey"
							screentip="Import NRC survey"
							size="large"
							supertip="Convert NRC patient satisfaction survey to SQL format" />
				</group>
				<group id ="SviGroup"
					   label="SVI">
					<button id="addSVI"
							getImage="sviButton_GetImage"
							label="Add SVI"
							onAction="OnAddSVI"
							screentip="Looks up Social Vulnerability Index"
							size="large"
							supertip="Gets Census Tract from address, then looks up SVI from .csv file" />
				</group>
				<group id ="TimecardGroup"
					   label="Monthly Report">
					<button id="extendTimecard"
							getImage="extendTimecard_GetImage"
							label="Extend monthly report"
							onAction="OnExtendTimecard"
							screentip="Extends monthly report to new month"
							size="large"
							supertip="Extends monthly report to new month" />
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>

