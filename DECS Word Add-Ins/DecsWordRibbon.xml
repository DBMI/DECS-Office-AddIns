<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab id="DecsTab" label="DECS Tools">
        <group id="SqlGroup"
               label="SQL">
			<button id="extractIcdButton" 
					getImage="icdExtractButton_GetImage" 
					label="Extract ICD"
					onAction="OnExtractICD"
					screentip="Extract ICD Codes"
					size="large"
					supertip="Extract ICD Codes to SQL code." />
			<button id="buildMrnsButton"
					getImage="buildMrnsButton_GetImage"
					label="Build MRN import"
					onAction="OnBuildMRN"
					screentip="Build MRN import code"
					size="large"
					supertip="Build SQL code that imports MRN list." />
		</group>
		  <group id ="SowGroup"
				 label="Project Setup">
			  <button id="scopeOfWorkParserButton"
					  getImage="scopeOfWorkParserButton_GetImage"
					  label="Parse Scope of Work"
					  onAction="OnParseSOW"
					  screentip="Set up project from Scope of Work (SOW) document."
					  size="large"
					  supertip="Parse Scope of Work (SOW) document and set up DECS project." />
		  </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>

